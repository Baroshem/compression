{
  "name": "@nuxt-modules/lazy-load",
  "version": "0.1.0",
  "description": "Lazy load module for Nuxt 3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt-modules/lazy-load.git"
  },
  "author": "Jakub Andrzejewski",
  "contributors": [
    "Baroshem <jakub.andrzejewski.dev@gmail.com>"
  ],
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/module.cjs",
      "import": "./dist/module.mjs"
    }
  },
  "main": "dist/module.mjs",
  "module": "dist/module.mjs",
  "types": "dist/module.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "nuxt dev example",
    "docs": "cd docs && yarn dev",
    "lint": "eslint --ext .js,.ts,.vue .",
    "prepublishOnly": "yarn build",
    "release": "yarn lint && standard-version && git push --follow-tags"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^8.0.0",
    "eslint": "^8.4.1",
    "nuxt3": "3.0.0-27324955.23397e6",
    "standard-version": "^9.3.2",
    "typescript": "^4.5.4",
    "unbuild": "0.6.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/nuxt-modules/lazy-load/issues"
  },
  "homepage": "https://github.com/nuxt-modules/lazy-load#readme",
  "directories": {
    "doc": "docs",
    "example": "example"
  },
  "keywords": [
    "lazy-load",
    "image",
    "performance",
    "nuxt",
    "module"
  ],
  "dependencies": {
    "lozad": "^1.16.0"
  }
}
